						"exec": [
							"// add varification for status code\r",
							"pm.test(\"Status code is 200 for <member-of> request\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"var responseData = pm.response.json();\r",
							"console.log(responseData);\r",
							"\r",
							"pm.test(\"verify name equals to <Personal Account> for member-of\", function(){\r",
							"    pm.expect(responseData[1].name, \"Personal Account\");\r",
							"});\r",
							"\r",
							"pm.test(\"verify description equals to <Personal Account> for member-of\", function(){\r",
							"    pm.expect(responseData[1].description, \"Personal Account\");\r",
							"});\r",
							"\r",
							"pm.test(\"verify id equals to <Personal Account> for member-of\", function(){\r",
							"    pm.expect(responseData[1].id, \"Personal Account\");\r",
							"});\r",
							"\r",
							"pm.test(\"verify userId equals to <Personal Account> for member-of\", function(){\r",
							"    pm.expect(responseData[1].userId, \"Personal Account\");\r",
							"});\r",
							"\r",
							"\r",
							"console.log(responseData[1].id)\r",
							"//Name the environment variable: workspaceId\r",
							"postman.setEnvironmentVariable(\"workspaceId\", responseData[1].id )\r",
							"\r",
							"postman.setEnvironmentVariable(\"userId\", responseData[1].userId)\r",
